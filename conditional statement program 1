#include <stdio.h>

int main() {

    double N;
    printf("Enter a number: ");
    scanf("%lf", &N);
    if (N <= 0.0) {
        if (N == 0.0)
            printf("You entered 0.");
        else
            printf("You entered a negative number.");
    } 
    else
        printf("You entered a positive number.");

    return 0;
}

#include <stdio.h>
int main() {
    int a,b;
    int num;
    printf("Enter two integers: ");
    scanf("%d", &a);
    scanf("%d",&b);
    num=a+b;

    // true if num is perfectly divisible by 2
    if(num % 2 == 0)
        printf("%d is even.", num);
    else
        printf("%d is odd.", num);
    
    return 0;
}
